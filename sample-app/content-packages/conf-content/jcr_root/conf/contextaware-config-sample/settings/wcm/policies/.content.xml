<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <wcm jcr:primaryType="nt:unstructured">
        <foundation jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <responsivegrid jcr:primaryType="nt:unstructured">
                    <root-container
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Root Container"
                        sling:resourceType="wcm/core/components/policy/policy"
                        components="[/libs/wcm/foundation/components/responsivegrid,group:Context-Aware Configuration Sample]">
                        <jcr:content
                            jcr:primaryType="nt:unstructured"
                            jcr:mixinTypes="[cq:ReplicationStatus]"
                            cq:lastReplicated="{Date}2021-12-22T00:00:00.000+01:00"
                            cq:lastReplicationAction="Activate"/>
                    </root-container>
                    <content-container
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Content Container"
                        sling:resourceType="wcm/core/components/policy/policy"
                        components="group:Context-Aware Configuration Sample">
                        <jcr:content
                            jcr:primaryType="nt:unstructured"
                            jcr:mixinTypes="[cq:ReplicationStatus]"
                            cq:lastReplicated="{Date}2021-12-22T00:00:00.000+01:00"
                            cq:lastReplicationAction="Activate"/>
                    </content-container>
                </responsivegrid>
                <page jcr:primaryType="nt:unstructured">
                    <page-policy
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Page Policy"
                        sling:resourceType="wcm/core/components/policy/policy"
                        clientlibs="contextaware-config-sample">
                        <jcr:content
                            jcr:primaryType="nt:unstructured"
                            jcr:mixinTypes="[cq:ReplicationStatus]"
                            cq:lastReplicated="{Date}2021-12-22T00:00:00.000+01:00"
                            cq:lastReplicationAction="Activate"/>
                    </page-policy>
                </page>
            </components>
        </foundation>
    </wcm>
</jcr:root>
